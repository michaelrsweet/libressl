<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
    <metadata>
        <id>libressl_native.redist</id>
        <title>LibreSSL for VS2019+ Redist</title>
        <version>3.5.3</version>
        <authors>Michael R Sweet, LibreSSL, OpenSSL</authors>
        <owners>michaelrsweet</owners>
        <projectUrl>https://www.libressl.org/</projectUrl>
        <license type="expression">ISC</license>
        <readme>build/native/README.md</readme>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>LibreSSL is a version of the TLS/crypto stack forked from OpenSSL in 2014, with goals of modernizing the codebase, improving security, and applying best practice development processes. This package contains the redistributable DLLs.</description>
        <copyright>Copyright (c) 1998-2011 The OpenSSL Project. Copyright (C) 1995-1998 Eric Young.</copyright>
        <tags>tls native</tags>
    </metadata>
    <files>
        <file src="README.md" target="build\native" />
        <file src="crypto\Debug\*.dll" target="build\native\bin\x64\Debug" />
        <file src="crypto\Release\*.dll" target="build\native\bin\x64\Release" />
        <file src="ssl\Debug\*.dll" target="build\native\bin\x64\Debug" />
        <file src="ssl\Release\*.dll" target="build\native\bin\x64\Release" />
        <file src="tls\Debug\*.dll" target="build\native\bin\x64\Debug" />
        <file src="tls\Release\*.dll" target="build\native\bin\x64\Release" />
    </files>
</package>
